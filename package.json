{
  "name": "xtal-side-nav",
  "version": "0.0.45",
  "keywords": [
    "web-components",
    "web-component",
    "custom-element",
    "custom-elements",
    "JSON",
    "Editor"
  ],
  "description": "Side nav (drawer) web component.",
  "main": "xtal-side-nav.js",
  "module": "xtal-side-nav.js",
  "exports": {
    ".": "./xtal-side-nav.js",
    "./xtal-side-nav.js": "./xtal-side-nav.js"
  },
  "types": "types.d.ts",
  "files": [
    "*.*"
  ],
  "scripts": {
    "serve": "node node_modules/xtal-test/serve.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package-check": "package-check",
    "doc": "custom-elements-manifest analyze",
    "build": "copy xsn-config.js xsn-config.mjs && node xsn-config.mjs > xtal-side-nav.js",
    "watch": "npm-watch"
  },
  "watch": {
    "build": "*-config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/xtal-side-nav.git"
  },
  "dependencies": {
    "trans-render": "0.0.441",
    "on-to-me": "0.0.49",
    "be-active": "0.0.11",
    "be-definitive": "0.0.21",
    "be-hive": "0.0.15",
    "be-loaded": "0.0.26",
    "be-noticed": "0.0.40",
    "may-it-be": "0.0.10"
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "xtal-test": "0.0.54",
    "@custom-elements-manifest/analyzer": "0.5.7",
    "npm-watch": "0.11.0"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/xtal-side-nav/issues"
  },
  "homepage": "https://github.com/bahrus/xtal-side-nav#readme",
  "customElements": "custom-elements.json"
}
