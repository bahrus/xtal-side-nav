{
  "name": "xtal-side-nav",
  "version": "0.0.100",
  "keywords": [
    "web-components",
    "web-component",
    "custom-element",
    "custom-elements",
    "JSON",
    "Editor"
  ],
  "description": "Side nav (drawer) web component.",
  "main": "xtal-side-nav.js",
  "module": "xtal-side-nav.js",
  "exports": {
    ".": "./xtal-side-nav.js",
    "./xtal-side-nav.js": "./xtal-side-nav.js"
  },
  "types": "types.d.ts",
  "files": [
    "*.*"
  ],
  "scripts": {
    "serve": "node node_modules/may-it-serve/serve.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package-check": "package-check",
    "build-dfn": "node xsn-config.mjs -dfn > xtal-side-nav.dfn.json",
    "build-js": "node xsn-config.mjs -js > xtal-side-nav.js",
    "build-html": "node xsn-config.mjs -html > xtal-side-nav.html",
    "build-manifest": "node doc.mjs > custom-elements.json",
    "watch": "npm-watch"
  },
  "watch": {
    "build-dfn": "*-config.mjs",
    "build-js": "*-config.mjs",
    "build-html": "*-config.mjs",
    "build-manifest": "types.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahrus/xtal-side-nav.git"
  },
  "dependencies": {
    "be-active": "0.0.47",
    "be-definitive": "0.0.75",
    "be-hive": "0.0.77",
    "be-loaded": "0.0.93",
    "may-it-be": "0.0.171",
    "trans-render": "0.0.646",
    "xtal-element": "0.0.540"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "0.6.4",
    "@skypack/package-check": "0.2.2",
    "be-importing": "0.0.41",
    "npm-watch": "0.11.0",
    "may-it-serve": "0.0.4"
  },
  "author": "anderson.bruce.b@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahrus/xtal-side-nav/issues"
  },
  "homepage": "https://github.com/bahrus/xtal-side-nav#readme",
  "customElements": "custom-elements.json"
}
